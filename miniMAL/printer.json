      ["`", ")"]],
  ["if", ["=", ["`", "string"], ["type", "exp"]],
    ["if", "print_readably",
      ["str", ["`", "\""],
              [".",
                [".",
                  [".", "exp",
                        ["`", "replace"], ["RegExp", ["`", "\\\\"], ["`", "g"]], ["`", "\\\\"]],
                  ["`", "replace"], ["RegExp", ["`", "\""], ["`", "g"]], ["`", "\\\""]],
                ["`", "replace"], ["RegExp", ["`", "\n"], ["`", "g"]], ["`", "\\n"]],
              ["`", "\""]],
